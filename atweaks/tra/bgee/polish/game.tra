@511 = ~Czar ten leczy istotę, na którą go rzucono, ze wszystkich chorób jakie ją trawią. Ponadto neutralizuje ślepotę, głuchotę i ogłupienie.~
@512 = ~Czar usuwa truciznę z organizmu istoty.~
@513 = ~Czar ten przywraca energię życiową istoty, która została jej pozbawiona (wszystkie utracone poziomy zostają przywrócone).~
@514 = ~Czar leczy wszystkich członków drużyny, przywracając im 1K8 +1 pkt na każdy poziom rzucającego (maksymalnie 20 pkt).~



// Zasady PnP dla biesów


@651 = ~Giń Baatezu!~
@652 = ~Bądź świadkiem prawdziwej potęgi!~
@653 = ~Na końcu... może być tylko chaos!~
@654 = ~Pożywię się twoją duszą, śmiertelniku!~
@655 = ~Nie ma ucieczki!~
@656 = ~Otchłań na ciebie czeka! Hahahaha!~
@657 = ~Ach, Wojna Krwi wzywa!~
@658 = ~Żałosny tanar'ri, zakończę twój marny żywot!~
@659 = ~Ha! Jesteś gorszy pod każdym możliwym względem.~
@660 = ~Nie ma dla ciebie nadziei, śmiertelniku! Twój czas nadszedł!~
@661 = ~Żałosna istoto, nie możesz mi nic zaoferować poza swoim życiem.~
@662 = ~Proszę bardzo, możesz spróbować uciec. To będzie o wiele zabawniejsze!~
@663 = ~Głupi śmiertelniku, twoje żałosne sztuczki mnie nie zwiodą!~
@664 = ~Fuj, magia śmiertelników jest taka przewidywalna.~
@665 = ~Co-?! Bogowie, pomóżcie!~
@666 = ~Nie, proszę! Aaaaaargh!~
@667 = ~Czy to jest... z-zło? Ratuj się kto może!~
@668 = ~D-demon!? Muszę uciekać!~
@669 = ~N-nie! Trzymaj się z daleka!~
