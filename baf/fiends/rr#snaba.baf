IF
	Die() // blocks using the Die trigger must be on top of a script
        Global("RR#DGaze","LOCALS",1)
        Global("RR#EACheck","LOCALS",1) // Fiend was summoned by an enemy
        Global("RR#XP","LOCALS",0)
THEN
	RESPONSE #100
                SetGlobal("RR#XP","LOCALS",1)
                SetGlobal("RR#DGaze","LOCALS",2)
                AddexperienceParty(16000) // Nabassu XP value
                ReallyForceSpellDeadRES("RR#DGZRM",Player1) // Death Gaze removal
                ReallyForceSpellDeadRES("RR#DGZRM",Player2) // Death Gaze removal
                ReallyForceSpellDeadRES("RR#DGZRM",Player3) // Death Gaze removal
                ReallyForceSpellDeadRES("RR#DGZRM",Player4) // Death Gaze removal
                ReallyForceSpellDeadRES("RR#DGZRM",Player5) // Death Gaze removal
                ReallyForceSpellDeadRES("RR#DGZRM",Player6) // Death Gaze removal
END

IF
	Die() // blocks using the Die trigger must be on top of a script
        Global("RR#EACheck","LOCALS",1) // Fiend was summoned by an enemy
        Global("RR#XP","LOCALS",0)
THEN
	RESPONSE #100
                SetGlobal("RR#XP","LOCALS",1)
                AddexperienceParty(16000) // Nabassu XP value
END


IF
  HasItem("dw#sumfi",Myself)
THEN
  RESPONSE #100
    DestroyItem("dw#sumfi")
    Continue()
END

IF
        Global("RR#EACheck","LOCALS",0)
	Allegiance(LastSummonerOf(Myself),GOODCUTOFF)
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
                SetGlobal("RR#EACheck","LOCALS",2)
		LeaveParty()
		ChangeEnemyAlly(Myself,ALLY)
                DialogueInterrupt(FALSE)
		SetInterrupt(TRUE)
END

IF
        Global("RR#EACheck","LOCALS",0)
	OR(2)
	  Allegiance(LastSummonerOf(Myself),EVILCUTOFF)
          !Exists(LastSummonerOf(Myself))
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
                SetGlobal("RR#EACheck","LOCALS",1)
		Enemy()
		SetInterrupt(TRUE)
END

IF
	OnCreation()
        Global("RR#EACheck","LOCALS",2)
