IF
	Die() // blocks using the Die trigger must be on top of a script
        Global("RR#EACheck","LOCALS",1) // Fiend was summoned by an enemy
        Global("RR#XP","LOCALS",0)
THEN
	RESPONSE #100
                SetGlobal("RR#XP","LOCALS",1)
                AddexperienceParty(26000) // Balor XP value
                Continue()
END

IF
	Die() // blocks using the Die trigger must be on top of a script
        Global("RR#DBFLE","LOCALS",0) // A Balor explodes if slain in the Abyss
	OR(10)
	  AreaCheck("AR0414") // Planar Sphere: Abyssal Pocket
	  AreaCheck("AR2900") // The Abyss
          AreaCheck("AR2901") // The Abyss (Fear Trial)
          AreaCheck("AR2902") // The Abyss (Pride Trial)
          AreaCheck("AR2903") // The Abyss (Greed Trial)
          AreaCheck("AR2904") // The Abyss (Selfishness Trial)
          AreaCheck("AR2905") // The Abyss (Wrath Trial)
          AreaCheck("AR2906") // The Abyss (unused?)
	  AreaCheck("AR4500") // Pocket Plane
	  AreaCheck("AR6200") // Throne of Bhaal
THEN
	RESPONSE #100
                SetGlobal("RR#DBFLE","LOCALS",1)
                ReallyForceSpellDeadRES("RR#DBFLE",Myself) // Death Throes
END


IF
  HasItem("dw#sumfi",Myself)
THEN
  RESPONSE #100
    DestroyItem("dw#sumfi")
    Continue()
END

IF
        Global("RR#EACheck","LOCALS",0)
	Allegiance(LastSummonerOf(Myself),GOODCUTOFF)
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
                SetGlobal("RR#EACheck","LOCALS",2)
		LeaveParty()
		ChangeEnemyAlly(Myself,ALLY)
                DialogueInterrupt(FALSE)
		SetInterrupt(TRUE)
END

