        !StateCheck(Myself,STATE_INVISIBLE)
	!See([0.0.DEMONIC.0.0.0.MASK_LAWFUL])
	!See(NearestEnemyOf(Myself))
	See([0.HUMANOID.0.INNOCENT])
THEN
	RESPONSE #100
		SetInterrupt(FALSE)
                SetGlobal("RR#GTempt","LOCALS",2)
                ReallyForceSpellRES("RR#PSTHA",LastSeenBy(Myself)) // Plane Shift - The Abyss
                ReallyForceSpellRES("RR#PSTHA",Myself) // Plane Shift - The Abyss
		SetInterrupt(TRUE)
END

