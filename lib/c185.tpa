COPY ~atweaks/eff/fey/rr#fcsng.eff~ ~override~                                     // Charming Song EFF 1 (for targeting only humanoids)
COPY ~atweaks/eff/fey/rr#fcsn2.eff~ ~override~                                     // Charming Song EFF 2 (for elven and half-elven charm immunity)
